CREATE TABLE `tblEmployee` (
	`EmployeeID`	INTEGER PRIMARY KEY AUTOINCREMENT,
	`Name`	TEXT,
	`City`	TEXT,
	`Department`	TEXT,
	`Gender`	TEXT
);


CREATE TABLE `tblCities` (
	`CityID`	INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
	`CityName`	TEXT
);


